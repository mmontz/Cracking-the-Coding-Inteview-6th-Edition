class solution{
public:
    void findSubsets (int n , vector <vector <int>>& all){
        if (n < 0)
            return;
        if (n == 0){
            all.push_back({});
            return;
        }
        findSubsets(n-1, all);
        
        for (int i=0 ; i<pow(2,n-1) ; i++){
            vector <int> temp = all[i];
            temp.push_back(n);
            all.push_back(temp);
        }
        return;
        
    }
    vector <vector <int>> findSubsets (int n){
        vector <vector <int>> res;
        findSubsets(n , res);
        return res;
    }
    
    void printSubsets (vector <vector <int>> &V){
        for (int i=0 ; i<V.size() ; i++){
            for (int j=0 ; j<V[i].size() ; j++){
                cout << V[i][j] << " ";
            }
            cout << endl;
        }
    }
};
